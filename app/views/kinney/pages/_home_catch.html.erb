<div class="container">
  <div class="row">
    <div id="more_clips">
      <h2><%= link_to t('kinney.more.video'), clips_path %></h2>
      <div class="row">
        <% @top_pick_clips.each do |clip| %>
          <div class="col-md-3 col-sm-3 col-xs-6 <%= cycle("odd", "even") -%>">
             <%= render :partial => '/kinney/clip_link', :locals => {:clip => clip} %>
          </div>
        <% end -%>
      </ul>
    </div>
  </div>
</div>